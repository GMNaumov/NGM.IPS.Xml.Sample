<?xml version="1.0" encoding="utf-8" ?>
<!--Корневой элемент файла с кратким описанием метаданных, передаваемых в процессе экспорта-импорта данных IPS-->
<METADATABRIEF>
	
	<!--Секция с описание типов атрибутов, параметров объектов и связей-->
	<ATTRIBUTE_TYPES>
		
		<!--!Обязательный атрибут! Уникальный идентификатор атрибута в документе XML-->
		<F_ATTRIBUTE_ID></F_ATTRIBUTE_ID>
		
		<!--!Обязательный атрибут! Наименование атрибута или параметра-->
		<F_NAME></F_NAME>
		
		<!--Псевдноним атрибута-->
		<F_ALIAS></F_ALIAS>
		
		<!--Глобальный идентификатор атрибута в системе IPS-->
		<F_GUID></F_GUID>
		
		<!--Тип данных атрибута-->
		<ATTRIBUTE_TYPE></ATTRIBUTE_TYPE>
	</ATTRIBUTE_TYPES>

	<!--Секция с информацией о типах объектов-->
	<OBJECT_TYPES>
		<!--!Обязательный атрибут! Уникальный идентификатор типа объекта в документе-->
		<OBJECT_TYPE></OBJECT_TYPE>
		
		<!--!Обязательный атрибут! Наименование типа объекта-->
		<F_OBJ_TYPE_NAME></F_OBJ_TYPE_NAME>
		
		<!--Глобальный идентификатор типа объекта в системе IPS-->
		<F_GUID></F_GUID>
		
		<!--Имя файла для иконки типа объекта-->
		<F_ICON></F_ICON>
	</OBJECT_TYPES>

	<!--Информация о типах связей-->
	<RELATION_TYPES>
		<!--!Обязательный атрибут! Уникальный идентификатор типа связи в документе-->
		<RELATION_TYPE></RELATION_TYPE>
		
		<!--!Обязательный атрибут! Наименование типа объекта-->
		<F_TYPE_NAME></F_TYPE_NAME>
		
		<!--Глобальный идентификатор типа объекта в системе IPS-->
		<F_GUID></F_GUID>
	</RELATION_TYPES>
</METADATABRIEF>
